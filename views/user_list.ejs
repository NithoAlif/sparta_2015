<% include layout/header %>

<table class="table table-stripped">
    <thead>
        <tr>
            <th>id</th>
            <th>email</th>
            <th>Nama Lengkap</th>
            <th>nim_tpb</th>
            <th>nim_jurusan</th>
        </tr>
    </thead>
    <tbody>
        <% user_list.forEach(function(user) { %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.email %></td>
                <td><%= user.fullname %></td>
                <td><%= user.nim_tpb %></td>
                <td><%= user.nim_jurusan %></td>
            </tr>
        <% }); %>
    </tbody>
</table>

<% include layout/footer %>